{
  "name": "mean-i3t",
  "description": "Use this scafolded app to build faster",
  "version": "0.0.1",
  "dependencies": {
    "async": "2.0.0",
    "body-parser": "1.15.2",
    "cookie-parser": "1.4.3",
    "express": "4.14.0",
    "fs-extra": "0.30.0",
    "glob": "7.0.5",
    "grunt": "1.0.1",
    "grunt-concurrent": "2.3.1",
    "grunt-env": "0.4.4",
    "grunt-nodemon": "0.4.2",
    "matchdep": "1.0.1",
    "method-override": "2.3.6",
    "moment": "2.14.1",
    "mongoose": "4.5.5",
    "morgan": "1.7.0",
    "request": "2.73.0",
    "underscore": "1.8.3",
    "winston": "2.2.0"
  },
  "devDependencies": {},
  "jshintConfig": {
    "bitwise": true,
    "camelcase": true,
    "eqeqeq": true,
    "esnext": true,
    "globalstrict": true,
    "indent": 2,
    "latedef": "nofunc",
    "maxcomplexity": 10,
    "maxdepth": 5,
    "maxlen": 80,
    "maxparams": 8,
    "maxstatements": 40,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonbsp": true,
    "predef": [
      "async",
      "ActErr",
      "logger",
      "util",
      "sendJSONResponse",
      "respondWithError",
      "config"
    ],
    "quotmark": "single",
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "white": true
  },
  "main": "server.js",
  "private": true
}
